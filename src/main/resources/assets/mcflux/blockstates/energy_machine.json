{
	"multipart": [
		{"when": {"variant": "energy_dist"}, "apply": {"model": "mcflux:energy_dist_core"}},
		{"when": {"variant": "energy_dist", "up": 1}, "apply": {"model": "mcflux:energy_dist_in"}},
		{"when": {"variant": "energy_dist", "up": 2}, "apply": {"model": "mcflux:energy_dist_out"}},
		{"when": {"variant": "energy_dist", "down": 1}, "apply": {"model": "mcflux:energy_dist_in", "x": 180}},
		{"when": {"variant": "energy_dist", "down": 2}, "apply": {"model": "mcflux:energy_dist_out", "x": 180}},
		{"when": {"variant": "energy_dist", "north": 1}, "apply": {"model": "mcflux:energy_dist_in", "x": 90}},
		{"when": {"variant": "energy_dist", "north": 2}, "apply": {"model": "mcflux:energy_dist_out", "x": 90}},
		{"when": {"variant": "energy_dist", "south": 1}, "apply": {"model": "mcflux:energy_dist_in", "x": 90, "y": 180}},
		{"when": {"variant": "energy_dist", "south": 2}, "apply": {"model": "mcflux:energy_dist_out", "x": 90, "y": 180}},
		{"when": {"variant": "energy_dist", "east": 1}, "apply": {"model": "mcflux:energy_dist_in", "x": 90, "y": 90}},
		{"when": {"variant": "energy_dist", "east": 2}, "apply": {"model": "mcflux:energy_dist_out", "x": 90, "y": 90}},
		{"when": {"variant": "energy_dist", "west": 1}, "apply": {"model": "mcflux:energy_dist_in", "x": 90, "y": 270}},
		{"when": {"variant": "energy_dist", "west": 2}, "apply": {"model": "mcflux:energy_dist_out", "x": 90, "y": 270}}
	]
}